        carousel_controller
        {
            motor_parameters
            {
                theta_motor
                {
                    HomeDirection 1
                    MotorFullStepsPerRev 200
                    ; user units per full MOTOR revolution in 1/10th degree = 90.0 degrees of CAROUSEL (or plate) revolution
                    ; NOTE that end result movement (carousel or plate rotation movement) is 1/4 due to reduction gearing between motor and carousel
                    UnitsPerRev 900
                    ; gearing is 'effective' gearing through the reduction pulleys, not an internal motor gearhead
                    GearheadRatio 4
                    EncoderTicksPerRev 800
                    ; Calculate user units of deadband corresponding to 4 encoder ticks;
                    ; That calculation is: U = 4 * (UnitsPerRev/EncoderTicksPerRev)
                    ; for 800 EncoderTicksPerRev, 4 encoder ticks of deadband calculates to 5 user units when rounded up to ensure full integer value encoder ticks
                    Deadband 10
                    InvertedDirection 1
                    StepSize 8
                    Acceleration 1200
                    DeAcceleration 1200
                    MinSpeed 0
                    MaxSpeed 150
                    ; NOTE: the overcurrent internal register is only 4-bits over the range of 0 to 6.0 amps, yielding steps of 0.375A
                    ; multiples of .375A (375000 uA) should be used, and may result in values potentially higher than the motor's rated current
                    ; 1.5A rated value
                    OverCurrent 2625000
                    StallCurrent 0
                    ; NOTE: the Hold-, Run-, Accel-, and Decel- current internal registers have a resolution of 0.03125amps (31.25 mA)
                    ; multiples of .03125A (31250 uA) should be used; Accel and Decel values should ALWAYS be more than 40% less than
                    ; the overcurrent value, as the overcurrent value may effectively vary by +/- 20% from the nominal setting
                    ; NOTE: the minimum allowable hold voltage divider value appears to be 31250.
                    ; Values less than this cause the controller chip to report an overcurrent condition at the end of move operations, including the 'home' command.
                    HoldVoltageDivide 750000
;                    HoldVoltageDivide 60000
                    RunVoltageDivide 1500000
                    AccVoltageDivide 1500000
                    DecVoltageDivide 1500000
                    CONFIG 44696
                    INT_SPEED 0
                    ST_SLP 25
                    FN_SLP_ACC 41
                    FN_SLP_DEC 41
                    HomingStepSize 0
                    DelayAfterMove 0
                    ProbeSpeed1 0
                    ProbeCurrent1 0
                    ProbeAbovePosition 0
                    ProbeSpeed2 0
                    ProbeCurrent2 0
                    ProbeRaise 0
                    MaxTravelPosition 0
                    MaxMoveRetries 0

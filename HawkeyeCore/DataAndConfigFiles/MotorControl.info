        focus_controller
        {
            motor_parameters
            {
                focus_motor
                {
                    HomeDirection 0
                    MotorFullStepsPerRev 200
                    ; user units per revolution in 1/10th microns = 0.3 mm per revolution or 300.0 microns
                    ; NOTE: the minimum user unit resolution possible is 0.1875 micron per motor micro-step using 8 micro-steps per full step
                    UnitsPerRev 3000
                    GearheadRatio 1
                    ; NOTE: motors that do not have encoders must use (EncoderStepsPerRev * StepSize) as their pseudo encoder count
                    EncoderTicksPerRev 1600
                    ; NOTE: the deadband represents 0.4 micron, which translates to 2.133 motor micro-steps;
                    ; Since motor steps must be an interger value, the actual deadband is 0.375 microns;
                    ; Reducing the deadband any further may result in the inability to hit the desired location
                    Deadband 4
                    InvertedDirection 0
                    StepSize 8
                    Acceleration 7500
                    DeAcceleration 7500
                    MinSpeed 0
                    MaxSpeed 2500
                    ; NOTE: the overcurrent internal register is only 4-bits over the range of 0 to 6.0 amps, yielding steps of 0.375A
                    ; multiples of .375A (375000 uA) should be used, and may result in values potentially higher than the motor's rated current
                    ; 0.5A rated value (needs to be spec'd at 0.75A)
                    OverCurrent 1125000
                    StallCurrent 0
                    ; NOTE: the Hold-, Run-, Accel-, and Decel- current internal registers have a resolution of 0.03125amps (31.25 mA)
                    ; multiples of .03125A (31250 uA) should be used; Accel and Decel values should ALWAYS be more than 40% less than
                    ; the overcurrent value, as the overcurrent value may effectively vary by +/- 20% from the nominal setting
                    ; NOTE: the minimum allowable hold voltage divider value appears to be 31250.
                    ; Values less than this cause the controller chip to report an overcurrent condition at the end of move operations, including the 'home' command.
                    HoldVoltageDivide 32000
                    RunVoltageDivide 500000
                    AccVoltageDivide 500000
                    DecVoltageDivide 500000
                    CONFIG 44696
                    INT_SPEED 0
                    ST_SLP 25
                    FN_SLP_ACC 41
                    FN_SLP_DEC 41
                    HomingStepSize 0
                    DelayAfterMove 0
                    ProbeSpeed1 0
                    ProbeCurrent1 0
                    ProbeAbovePosition 0
                    ProbeSpeed2 0
                    ProbeCurrent2 0
                    ProbeRaise 0
                    MaxTravelPosition 140000
                    MaxMoveRetries 0
                }
            }
            controller_config
            {
                FocusMaxTravel 180000
                PositionTolerance 6
                FocusStartPos 105000
                FocusMaxPos 140000
                ; user units per coarse step = 0.03 mm per step (30 micron), representing 160 motor microsteps (20 full motor steps)
                FocusCoarseStepValue 300
                ; user units per fine step = 0.0015 mm per step (1.5 micron), representing 8 motor micro-steps (1 full motor step)
